{
  "name": "doganaylab-api-translate",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ext": "web-ext build --overwrite-dest",
    "build:ext:firefox": "web-ext build --source-dir=dist-firefox --overwrite-dest",
    "build:ext:chrome": "cd dist-chrome && zip -r ../web-ext-artifacts/chrome-v3.0.0.zip .",
    "build:chrome-old": "bash build-chrome.sh",
    "build:chrome": "NODE_ENV=production webpack --config webpack/webpack.chrome.cjs",
    "build:firefox": "NODE_ENV=production webpack --config webpack/webpack.firefox.cjs",
    "build:all": "npm run build:chrome && npm run build:firefox",
    "package:firefox": "npm run build:firefox && npm run build:ext:firefox",
    "package:chrome": "npm run build:chrome && mkdir -p web-ext-artifacts && npm run build:ext:chrome",
    "package:all": "npm run build:all && npm run package:firefox && npm run package:chrome",
    "dev:chrome": "webpack --config webpack/webpack.chrome.cjs --watch",
    "dev:firefox": "webpack --config webpack/webpack.firefox.cjs --watch",
    "clean:chrome": "rm -rf dist-chrome",
    "clean:firefox": "rm -rf dist-firefox",
    "clean:all": "npm run clean:chrome && npm run clean:firefox",
    "lint": "eslint .",
    "lint:ext": "web-ext lint --source-dir=dist-firefox",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "web-ext": "^9.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.1.24",
    "@types/firefox-webext-browser": "^143.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
